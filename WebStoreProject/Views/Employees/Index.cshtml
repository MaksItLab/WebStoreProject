@model IEnumerable<WebStoreProject.Models.Employees>



<div class="container">
	<table class="table">
			<thead>
				<tr>
					<th>ID</th>
					<th>FirstName</th>
					<th>LastName</th>
					<th>Patronymic</th>
					<th>Age</th>
					<th>Link</th>
				</tr>
			</thead>
			<tbody>
			@foreach (var item in Model)
			{
								<tr>
									<td>@item.Id</td>
									<td>@item.FirstName</td>
									<td>@item.LastName</td>
									<td>@item.Patronymic</td>
									<td>@item.Age</td>
									<td>
										<a asp-controller="Employees" asp-action="Details" asp-route-index="@item.Id">инфо</a>
										<a asp-controller="Employees" asp-action="Edit" asp-route-id="@item.Id">edit</a>
										<a asp-controller="Employees" asp-action="Delete" asp-route-id="@item.Id" class="text-danger">delete</a>
									</td>
								</tr>
			}
			</tbody>
		</table>

		<a href="/">Главная страница</a>
</div>